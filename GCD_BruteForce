package DSA;

public class GCD_Calculate {

    public static int findGCD(int n1, int n2){
        int gcd = 1;
        for(int i=2; i<=Math.min(n1, n2); i++){
            if(n1%i == 0 & n2%i == 0){
                gcd = i;
            }
        }
        return gcd;
    }
    public static void main(String[] args) {
        int n1 = 12;
        int n2 = 18;

        int gcd = findGCD(n1, n2);
        System.out.println("GCD of " +n1 + " and " +n2 + " is: " + gcd);
    }
}
